#include<stdio.h>
int count=0;
void Factor(int num) {
 {
    int i;
    for (i = 1; i <= num;++i)
    {
        count++;
     if (num % i== 0)
        {
            count++;
          
        }  
        count++;
     } 
     count++;
  }
}
 int main(){
     int n;
     scanf("%d",&n);
     Factor(n);
     printf("%d",count);
 }
